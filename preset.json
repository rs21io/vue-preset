{
  "useConfigFiles": true,
  "plugins": {
    "@vue/cli-plugin-babel": {},
    "@vue/cli-plugin-typescript": {
      "classComponent": true,
      "useTsWithBabel": true
    },
    "@vue/cli-plugin-router": {
      "historyMode": true
    },
    "@vue/cli-plugin-vuex": {},
    "@vue/cli-plugin-eslint": {
      "config": "prettier",
      "lintOn": ["save"]
    },
    "vue-cli-plugin-vuetify": {
      "preset": "configure",
      "replaceComponents": true,
      "iconFont": "mdi",
      "installFonts": false,
      "locale": "en",
      "useAlaCarte": true,
      "useCustomProperties": false,
      "usePolyfill": false,
      "useTheme": false,
      "useV3": false,
      "useVite": false
    },
    "@vue/cli-plugin-unit-jest": {},
    "@vue/cli-plugin-e2e-cypress": {}
  },
  "vueVersion": "2",
  "cssPreprocessor": "dart-sass"
}
